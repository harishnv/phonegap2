<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.ttt.archiveapp"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name> Ask A Question</name>

    <description>
       Ask A Question
    </description>

<icon src="{icon}" />
<platform name="ios">
  <icon src="themes/images/logo159_159.png" width="57" height="57" />
  <icon src="themes/images/logo159_159.png" width="72" height="72" />
  <icon src="themes/images/logo159_159.png" width="114" height="114" />
  <icon src="themes/images/logo159_159.png" width="144" height="144" />
</platform>
 
<platform name="android">
  <icon src="themes/images/logo159_159.png" gap:density="ldpi" />
  <icon src="themes/images/logo159_159.png" gap:density="mdpi" />
  <icon src="themes/images/logo159_159.png" gap:density="hdpi" />
  <icon src="themes/images/logo159_159.png" gap:density="xhdpi" />
</platform>


    <author href="https://build.phonegap.com" email="support@phonegap.com">
       Harish Satpute
    </author>
<plugins>
    <plugin name="Device" value="org.apache.cordova.Device"/>
</plugins>
</widget>
